script: |
  super -f tsv -I q1957.sql | sort > q1957.out

inputs:
  - name: q1957.sql
    data: |
      SELECT CASE + 86 WHEN + 69 + + CAST ( NULL AS INTEGER ) THEN - + MAX ( + + NULLIF ( 62, + - 87 ) ) ELSE + CAST ( - MAX ( - + 24 ) AS INTEGER ) END;

outputs:
  - name: q1957.out
    data: |
      24
