    data: |
      SELECT + + NULLIF ( + 24, - 36 ) * - + CAST ( NULL AS INTEGER ) + + - COALESCE ( - 61, + 97, - ( + 24 ) ), + CASE + 87 WHEN + + MAX ( + - 24 ) THEN NULL WHEN 61 THEN - + 99 * - + ( - - CAST ( NULL AS INTEGER ) ) - - 51 ELSE NULL END AS col2;

outputs:
  - name: q15012.out
    data: |
      NULL	NULL
