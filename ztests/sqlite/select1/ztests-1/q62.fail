# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q62.sql > q62.out

vector: true

inputs:
  - name: q62.sql
    data: |
      SELECT (a+b+c+d+e)/5,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END,
      c-d,
      a+b*2+c*3+d*4,
      a+b*2+c*3+d*4+e*5
      FROM t1
      WHERE (e>a AND e<b)
      AND c>d
      ORDER BY 2,5,6,4,1,3;
  - name: t1

outputs:
  - name: q62.out
    data: |
      117	3	1180	3	1172	1757
      192	18	382	3	1918	2878
      222	24	440	2	2226	3331
