# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q330.sql > q330.out

inputs:
  - name: q330.sql
    data: |
      SELECT b-c,
      (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d)
      FROM t1
      WHERE (c<=d-2 OR c>=d+2)
      AND (e>c OR e<d)
      AND c>d
      ORDER BY 2,1;
  - name: t1

outputs:
  - name: q330.out
    data: |
      -3	0
      -1	0
      -1	0
