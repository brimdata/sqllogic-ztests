# Failed due to: https://github.com/brimdata/super/issues/6549
script: |
  super -f tsv -I q800.sql > q800.out

vector: true

inputs:
  - name: q800.sql
    data: |
      SELECT a+b*2+c*3+d*4,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b)
      FROM t1
      WHERE e+d BETWEEN a+b-10 AND c+130
      AND d>e
      AND b>c
      ORDER BY 2,1;
  - name: t1

outputs:
  - name: q800.out
    data: |
      1226	4
      1272	5
