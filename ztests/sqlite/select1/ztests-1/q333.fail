script: |
  super -f tsv -I q333.sql > q333.out

inputs:
  - name: q333.sql
    data: |
      SELECT d-e,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      CASE WHEN a<b-3 THEN 111 WHEN a<=b THEN 222
      WHEN a<b+3 THEN 333 ELSE 444 END
      FROM t1
      WHERE e+d BETWEEN a+b-10 AND c+130
      ORDER BY 3,2,1;
  - name: t1

outputs:
  - name: q333.out
    data: |
      4	2	222
      -1	3	222
      2	4	222
      2	5	222
      -1	1	333
      -2	0	444
