      SELECT b-c,
      a+b*2,
      d,
      abs(b-c)
      FROM t1
      WHERE EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b)
      AND b>c
      AND (c<=d-2 OR c>=d+2);
  - name: t1

