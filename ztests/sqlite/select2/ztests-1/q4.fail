# Failed due to: https://github.com/brimdata/super/issues/6549
script: |
  super -f tsv -I q4.sql | sort > q4.out

vector: true

inputs:
  - name: q4.sql
    data: |
      SELECT (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d),
      d-e
      FROM t1
      WHERE e+d BETWEEN a+b-10 AND c+130
      OR (a>b-2 AND a<b+2);
  - name: t1

outputs:
  - name: q4.out
    data: |
      0	-1
      0	-1
      0	-1
      0	-4
      0	1
      0	1
      0	2
      0	3
      0	NULL
      0	NULL
