# Failed due to: https://github.com/brimdata/super/issues/6549
script: |
  super -f tsv -I q749.sql | sort > q749.out

vector: true

inputs:
  - name: q749.sql
    data: |
      SELECT a,
      (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d),
      (a+b+c+d+e)/5,
      a+b*2+c*3+d*4,
      abs(b-c)
      FROM t1
      WHERE a IS NULL
      AND coalesce(a,b,c,d,e)<>0;
  - name: t1

outputs:
  - name: q749.out
    data: |
      NULL	0	NULL	NULL	1
      NULL	0	NULL	NULL	2
