# Failed due to: https://github.com/brimdata/super/issues/6549
script: |
  super -f tsv -I q700.sql | sort > q700.out

vector: true

inputs:
  - name: q700.sql
    data: |
      SELECT c-d,
      (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d),
      d-e,
      (a+b+c+d+e)/5
      FROM t1
      WHERE EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b)
      AND b>c
      AND a IS NULL;
  - name: t1

outputs:
  - name: q700.out
    data: |
