    data: |
      SELECT d-e
      FROM t1
      WHERE EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b)
      AND coalesce(a,b,c,d,e)<>0
      AND c BETWEEN b-2 AND d+2;
  - name: t1

outputs:
  - name: q89.out
    data: |
      -2
      -4
      -4
      1
      1
      1
      2
      2
      2
      3
      3
      4
      NULL
      NULL
