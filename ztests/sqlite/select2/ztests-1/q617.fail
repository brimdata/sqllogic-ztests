# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q617.sql | sort > q617.out

inputs:
  - name: q617.sql
    data: |
      SELECT abs(b-c),
      c-d,
      d-e,
      e,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b)
      FROM t1
      WHERE (a>b-2 AND a<b+2)
      AND d>e
      AND b IS NOT NULL;
  - name: t1

outputs:
  - name: q617.out
    data: |
      2	1	1	135	5
      3	1	3	180	14
      4	1	1	132	4
