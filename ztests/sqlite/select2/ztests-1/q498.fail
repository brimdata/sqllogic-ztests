    data: |
      SELECT (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d)
      FROM t1
      WHERE b>c
      AND (e>c OR e<d)
      AND coalesce(a,b,c,d,e)<>0;
  - name: t1

outputs:
  - name: q498.out
    data: |
      0
      0
      0
      0
      0
      0
      0
      0
      0
