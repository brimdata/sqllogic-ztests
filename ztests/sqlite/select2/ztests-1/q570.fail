# Failed due to: https://github.com/brimdata/super/issues/6549
script: |
  super -f tsv -I q570.sql | sort > q570.out

vector: true

inputs:
  - name: q570.sql
    data: |
      SELECT b,
      d-e,
      a+b*2+c*3+d*4,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      CASE WHEN a<b-3 THEN 111 WHEN a<=b THEN 222
      WHEN a<b+3 THEN 333 ELSE 444 END
      FROM t1
      WHERE a IS NULL
      AND d>e;
  - name: t1

outputs:
  - name: q570.out
    data: |
      112	4	NULL	1	444
