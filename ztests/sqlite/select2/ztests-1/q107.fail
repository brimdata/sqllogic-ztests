# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q107.sql | sort > q107.out

inputs:
  - name: q107.sql
    data: |
      SELECT b-c,
      CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END,
      d-e
      FROM t1
      WHERE EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b)
      AND c>d
      AND a>b;

outputs:
  - name: q107.out
    data: |
      -1	376	-4
      -2	1700	-2
      -3	364	3
      -3	426	2
      -4	1300	1
