# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q1432.sql | sort > q1432.out

vector: true

inputs:
  - name: q1432.sql
    data: |
      SELECT c,
      a-b,
      b-c,
      (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d)
      FROM t1
      WHERE c BETWEEN b-2 AND d+2
      AND (e>a AND e<b)
      AND d>e;
  - name: t1

outputs:
  - name: q1432.out
    data: |
      224	-3	-1	0
      247	-4	2	0
