# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q2839.sql | sort > q2839.out

inputs:
  - name: q2839.sql
    data: |
      SELECT (a+b+c+d+e)/5,
      CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END
      FROM t1
      WHERE b>c
      AND d>e
      AND EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b)
      ORDER BY 1,2;
  - name: t1

outputs:
  - name: q2839.out
    data: |
      127	1290
      137	1390
      232	468
      247	490
