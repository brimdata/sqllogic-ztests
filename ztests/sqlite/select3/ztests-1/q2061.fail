# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q2061.sql | sort > q2061.out

inputs:
  - name: q2061.sql
    data: |
      SELECT a+b*2+c*3+d*4,
      abs(b-c),
      (a+b+c+d+e)/5,
      c
      FROM t1
      WHERE EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b)
      AND (c<=d-2 OR c>=d+2)
      AND b>c
      ORDER BY 4,3,2;

outputs:
  - name: q2061.out
    data: |
      1272	4	127	125
      1918	1	192	193
      2165	3	217	215
      2323	1	232	231
