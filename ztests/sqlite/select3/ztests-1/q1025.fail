# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q1025.sql | sort > q1025.out

inputs:
  - name: q1025.sql
    data: |
      SELECT (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d),
      abs(a),
      abs(b-c)
      FROM t1
      WHERE e+d BETWEEN a+b-10 AND c+130
      AND b>c
      ORDER BY 3,1;
  - name: t1

outputs:
  - name: q1025.out
    data: |
      0	127	4
