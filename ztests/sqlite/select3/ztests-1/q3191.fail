# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q3191.sql | sort > q3191.out

inputs:
  - name: q3191.sql
    data: |
      SELECT (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      e,
      a+b*2+c*3,
      abs(b-c)
      FROM t1
      WHERE e+d BETWEEN a+b-10 AND c+130
      AND a>b
      AND c>d
      ORDER BY 2,1,4;

outputs:
  - name: q3191.out
    data: |
