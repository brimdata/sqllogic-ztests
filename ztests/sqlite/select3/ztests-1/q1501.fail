# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q1501.sql | sort > q1501.out

inputs:
  - name: q1501.sql
    data: |
      SELECT a,
      d-e,
      c-d,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      b,
      c,
      abs(b-c)
      FROM t1
      WHERE a>b
      AND d NOT BETWEEN 110 AND 150
      AND (e>a AND e<b)
      ORDER BY 6,4;

outputs:
  - name: q1501.out
    data: |
