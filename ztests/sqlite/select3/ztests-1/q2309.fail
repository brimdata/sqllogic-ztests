# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q2309.sql | sort > q2309.out

inputs:
  - name: q2309.sql
    data: |
      SELECT abs(a),
      a+b*2+c*3+d*4,
      b,
      (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d),
      a+b*2+c*3+d*4+e*5,
      a-b,
      e
      FROM t1
      WHERE e+d BETWEEN a+b-10 AND c+130
      AND (e>c OR e<d)
      AND c>d
      ORDER BY 6,5,4,7,1,3;

outputs:
  - name: q2309.out
    data: |
