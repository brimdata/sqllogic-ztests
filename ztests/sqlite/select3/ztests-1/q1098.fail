script: |
  super -f tsv -I q1098.sql | sort > q1098.out

inputs:
  - name: q1098.sql
    data: |
      SELECT b,
      b-c,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      c-d,
      a+b*2+c*3+d*4
      FROM t1
      WHERE e+d BETWEEN a+b-10 AND c+130
      AND (a>b-2 AND a<b+2)
      AND (e>a AND e<b);

