script: |
  super -f tsv -I q1580.sql | sort > q1580.out

vector: true

inputs:
  - name: q1580.sql
    data: |
      SELECT a+b*2,
      a+b*2+c*3+d*4,
      d,
      CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END,
      c-d,
      a
      FROM t1
      WHERE a>b
      AND EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b)
      AND e+d BETWEEN a+b-10 AND c+130;
  - name: t1

outputs:
  - name: q1580.out
    data: |
