# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q3277.sql | sort > q3277.out

inputs:
  - name: q3277.sql
    data: |
      SELECT c-d,
      b-c,
      a,
      a+b*2+c*3
      FROM t1
      WHERE EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b)
      AND e+d BETWEEN a+b-10 AND c+130
      AND c>d
      ORDER BY 4,3,1,2;
  - name: t1

outputs:
  - name: q3277.out
    data: |
