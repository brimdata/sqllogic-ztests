# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q77.sql | sort > q77.out

vector: true

inputs:
  - name: q77.sql
    data: |
      SELECT a-b,
      a+b*2+c*3+d*4+e*5,
      b,
      abs(a),
      a
      FROM t1
      WHERE EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b)
      AND b>c
      AND e+d BETWEEN a+b-10 AND c+130
      ORDER BY 5,1,4,3,2;
  - name: t1

outputs:
  - name: q77.out
    data: |
      -2	1902	129	127	127
