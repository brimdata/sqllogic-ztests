# Failed due to: https://github.com/brimdata/super/issues/6549
script: |
  super -f tsv -I q2310.sql | sort > q2310.out

vector: true

inputs:
  - name: q2310.sql
    data: |
      SELECT abs(a),
      a+b*2+c*3+d*4,
      b,
      (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d),
      a+b*2+c*3+d*4+e*5,
      a-b,
      e
      FROM t1
      WHERE c>d
      AND (e>c OR e<d)
      AND e+d BETWEEN a+b-10 AND c+130;
  - name: t1

outputs:
  - name: q2310.out
    data: |
