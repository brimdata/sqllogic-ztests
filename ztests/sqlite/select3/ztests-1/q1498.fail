script: |
  super -f tsv -I q1498.sql | sort > q1498.out

inputs:
  - name: q1498.sql
    data: |
      SELECT a,
      d-e,
      c-d,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      b,
      c,
      abs(b-c)
      FROM t1
      WHERE (e>a AND e<b)
      AND a>b
      AND d NOT BETWEEN 110 AND 150;

