# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q1811.sql | sort > q1811.out

inputs:
  - name: q1811.sql
    data: |
      SELECT e,
      a,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      c-d,
      abs(a),
      d,
      b-c
      FROM t1
      WHERE (a>b-2 AND a<b+2)
      AND (c<=d-2 OR c>=d+2)
      ORDER BY 2,3,1,5,4,6,7;

outputs:
  - name: q1811.out
    data: |
