# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q931.sql | sort > q931.out

vector: true

inputs:
  - name: q931.sql
    data: |
      SELECT (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      abs(a),
      a+b*2+c*3,
      b-c,
      (a+b+c+d+e)/5,
      abs(b-c),
      c
      FROM t1
      WHERE d NOT BETWEEN 110 AND 150
      AND (a>b-2 AND a<b+2)
      AND a>b
      ORDER BY 1,3,4,5,6,2,7;
  - name: t1

outputs:
  - name: q931.out
    data: |
      14	182	1096	-3	182	3	184
      17	199	1180	3	197	3	195
      22	229	1360	3	227	3	225
