    data: |
      SELECT (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d),
      d
      FROM t1
      WHERE d NOT BETWEEN 110 AND 150
      AND d>e
      AND b>c
      ORDER BY 3,2,1;
  - name: t1

