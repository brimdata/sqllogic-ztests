# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q86.sql | sort > q86.out

inputs:
  - name: q86.sql
    data: |
      SELECT a,
      (a+b+c+d+e)/5,
      b
      FROM t1
      WHERE (e>c OR e<d)
      AND EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b)
      AND c>d;

