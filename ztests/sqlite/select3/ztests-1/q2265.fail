# Failed due to: https://github.com/brimdata/super/issues/6549
script: |
  super -f tsv -I q2265.sql | sort > q2265.out

vector: true

inputs:
  - name: q2265.sql
    data: |
      SELECT b-c,
      (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d),
      a+b*2
      FROM t1
      WHERE (c<=d-2 OR c>=d+2)
      AND (e>a AND e<b)
      AND a>b
      ORDER BY 3,1,2;
  - name: t1

outputs:
  - name: q2265.out
    data: |
