# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q1500.sql | sort > q1500.out

vector: true

inputs:
  - name: q1500.sql
    data: |
      SELECT a,
      d-e,
      c-d,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      b,
      c,
      abs(b-c)
      FROM t1
      WHERE a>b
      AND d NOT BETWEEN 110 AND 150
      AND (e>a AND e<b);
  - name: t1

outputs:
  - name: q1500.out
    data: |
