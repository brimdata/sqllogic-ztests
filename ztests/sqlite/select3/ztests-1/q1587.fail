script: |
  super -f tsv -I q1587.sql | sort > q1587.out

inputs:
  - name: q1587.sql
    data: |
      SELECT a+b*2,
      a+b*2+c*3+d*4,
      d,
      CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END,
      c-d,
      a
      FROM t1
      WHERE a>b
      AND e+d BETWEEN a+b-10 AND c+130
      AND EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b)
      ORDER BY 3,4;

