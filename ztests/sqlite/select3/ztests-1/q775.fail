# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q775.sql | sort > q775.out

vector: true

inputs:
  - name: q775.sql
    data: |
      SELECT b,
      (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d),
      a+b*2
      FROM t1
      WHERE d>e
      AND c BETWEEN b-2 AND d+2
      AND a>b
      ORDER BY 2,3,1;
  - name: t1

outputs:
  - name: q775.out
    data: |
      130	0	391
      160	0	483
      181	0	544
      211	0	635
      232	0	698
