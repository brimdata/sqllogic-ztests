script: |
  super -f tsv -I q1094.sql | sort > q1094.out

vector: true

inputs:
  - name: q1094.sql
    data: |
      SELECT b,
      b-c,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      c-d,
      a+b*2+c*3+d*4
      FROM t1
      WHERE (e>a AND e<b)
      AND e+d BETWEEN a+b-10 AND c+130
      AND (a>b-2 AND a<b+2);
  - name: t1

