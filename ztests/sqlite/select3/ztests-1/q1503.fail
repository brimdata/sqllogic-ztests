# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q1503.sql | sort > q1503.out

inputs:
  - name: q1503.sql
    data: |
      SELECT a,
      d-e,
      c-d,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      b,
      c,
      abs(b-c)
      FROM t1
      WHERE a>b
      AND (e>a AND e<b)
      AND d NOT BETWEEN 110 AND 150
      ORDER BY 1,4,3,7,5;
  - name: t1

outputs:
  - name: q1503.out
    data: |
