script: |
  super -f tsv -I q2843.sql | sort > q2843.out

inputs:
  - name: q2843.sql
    data: |
      SELECT (a+b+c+d+e)/5,
      CASE WHEN c>(SELECT avg(c) FROM t1) THEN a*2 ELSE b*10 END
      FROM t1
      WHERE d>e
      AND b>c
      AND EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b)
      ORDER BY 1,2;

outputs:
