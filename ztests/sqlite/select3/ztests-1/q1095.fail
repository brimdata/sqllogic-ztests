# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q1095.sql | sort > q1095.out

inputs:
  - name: q1095.sql
    data: |
      SELECT b,
      b-c,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      c-d,
      a+b*2+c*3+d*4
      FROM t1
      WHERE (e>a AND e<b)
      AND e+d BETWEEN a+b-10 AND c+130
      AND (a>b-2 AND a<b+2)
      ORDER BY 2,5;

outputs:
  - name: q1095.out
    data: |
