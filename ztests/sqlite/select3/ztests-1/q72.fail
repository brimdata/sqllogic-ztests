# Failed due to: https://github.com/brimdata/super/issues/6549
script: |
  super -f tsv -I q72.sql | sort > q72.out

vector: true

inputs:
  - name: q72.sql
    data: |
      SELECT a-b,
      a+b*2+c*3+d*4+e*5,
      b,
      abs(a),
      a
      FROM t1
      WHERE e+d BETWEEN a+b-10 AND c+130
      AND EXISTS(SELECT 1 FROM t1 AS x WHERE x.b<t1.b)
      AND b>c;
  - name: t1

outputs:
  - name: q72.out
    data: |
      -2	1902	129	127	127
