script: |
  super -f tsv -I q1430.sql | sort > q1430.out

vector: true

inputs:
  - name: q1430.sql
    data: |
      SELECT c,
      a-b,
      b-c,
      (SELECT count(*) FROM t1 AS x WHERE x.c>t1.c AND x.d<t1.d)
      FROM t1
      WHERE (e>a AND e<b)
      AND d>e
      AND c BETWEEN b-2 AND d+2;
  - name: t1

outputs:
  - name: q1430.out
    data: |
      224	-3	-1	0
      247	-4	2	0
