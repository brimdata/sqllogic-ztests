# Failed due to: https://github.com/brimdata/super/issues/6549
script: |
  super -f tsv -I q1497.sql | sort > q1497.out

vector: true

inputs:
  - name: q1497.sql
    data: |
      SELECT a,
      d-e,
      c-d,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      b,
      c,
      abs(b-c)
      FROM t1
      WHERE (e>a AND e<b)
      AND d NOT BETWEEN 110 AND 150
      AND a>b
      ORDER BY 3,5,4,7,6;
  - name: t1

outputs:
  - name: q1497.out
    data: |
