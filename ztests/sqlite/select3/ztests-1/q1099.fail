# Failed due to: https://github.com/brimdata/super/issues/6013
script: |
  super -f tsv -I q1099.sql | sort > q1099.out

vector: true

inputs:
  - name: q1099.sql
    data: |
      SELECT b,
      b-c,
      (SELECT count(*) FROM t1 AS x WHERE x.b<t1.b),
      c-d,
      a+b*2+c*3+d*4
      FROM t1
      WHERE e+d BETWEEN a+b-10 AND c+130
      AND (a>b-2 AND a<b+2)
      AND (e>a AND e<b)
      ORDER BY 4,1,3,5;
  - name: t1

outputs:
  - name: q1099.out
    data: |
